#!/bin/bash

schema='org.gnome.desktop.interface'

gsettings set "$schema" cursor-theme 'capitaine-cursors'
gsettings set "$schema" document-font-name 'Noto Sans 11'
gsettings set "$schema" font-name 'Noto Sans Medium 10'
gsettings set "$schema" gtk-theme 'Materia-dark-compact'
gsettings set "$schema" icon-theme 'Papirus-Dark'
gsettings set "$schema" monospace-font-name 'monospace'
gsettings set "$schema" toolbar-icons-size 'small'
